{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "tsconfig.json",
    ".prettierignore",
    ".env.*"
  ],
  "tasks": {
    "prepublishOnly": {},
    "build": {
      "dependsOn": [
        "test:ci",
        "prepublishOnly"
      ],
      "outputs": [
        ".svelte-kit/**",
        "dist/**"
      ]
    },
    "test:ci": {
      "outputs": [],
      "dependsOn": []
    },
    "coverage": {
      "outputs": [
        "coverage/**"
      ]
    },
    "lint": {
      "outputs": [],
      "dependsOn": []
    },
    "format": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "latest": {
      "outputs": []
    },
    "release": {
      "dependsOn": [
        "test:ci"
      ]
    }
  }
}
