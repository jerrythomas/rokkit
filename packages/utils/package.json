{
	"name": "@rokkit/utils",
	"version": "1.0.0-next.32",
	"description": "Assorted utility funtions and classes to simplify development.",
	"author": "Jerry Thomas <me@jerrythomas.name>",
	"license": "MIT",
	"svelte": "src/index.js",
	"module": "src/index.js",
	"main": "src/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepublishOnly": "tsc",
		"format": "prettier --write .",
		"lint": "eslint --fix .",
		"test:ct": "playwright test -c playwright.config.js",
		"test:ui": "vitest --ui",
		"test:ci": "vitest run",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"latest": "pnpm upgrade --latest && pnpm test:ci",
		"release": "tsc && pnpm publish --access public"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^2.4.2",
		"@testing-library/svelte": "^4.0.1",
		"@types/d3-array": "^3.0.5",
		"@types/d3-collection": "^1.0.10",
		"@types/ramda": "^0.29.2",
		"@vitest/ui": "~0.32.2",
		"c8": "^8.0.0",
		"js-yaml": "^4.1.0",
		"jsdom": "^22.1.0",
		"shared-config": "workspace:*",
		"svelte": "^4.0.0",
		"typescript": "^5.1.3",
		"validators": "workspace:*",
		"vite": "^4.3.9",
		"vitest": "~0.32.2"
	},
	"files": [
		"src/**/*.js",
		"src/**/*.svelte",
		"!src/fixtures",
		"!src/**/*.spec.js"
	],
	"exports": {
		"./src": "./src",
		"./string": "./src/string.js",
		"./story": "./src/story.js",
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./src/index.js"
		}
	},
	"dependencies": {
		"@lukeed/uuid": "^2.0.1",
		"d3-array": "^3.2.4",
		"d3-collection": "^1.0.7",
		"frontmatter": "^0.0.3",
		"ramda": "^0.29.0"
	}
}