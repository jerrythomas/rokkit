{
	"name": "@rokkit/utils",
	"version": "1.0.0-next.27",
	"description": "Assorted utility funtions and classes to simplify development.",
	"author": "Jerry Thomas <me@jerrythomas.name>",
	"license": "MIT",
	"svelte": "src/index.js",
	"module": "src/index.js",
	"main": "src/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"prettier": "@jerrythomas/prettier-config",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepublishOnly": "tsc",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"test:ct": "playwright test -c playwright.config.js",
		"test:ui": "vitest --ui",
		"test:ci": "vitest run",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"upgrade": "pnpm upgrade"
	},
	"devDependencies": {
		"@jerrythomas/prettier-config": "^1.0.0",
		"@sveltejs/vite-plugin-svelte": "^2.2.0",
		"@testing-library/svelte": "^3.2.2",
		"@types/d3-array": "^3.0.4",
		"@types/d3-collection": "^1.0.10",
		"@types/ramda": "^0.29.1",
		"@vitest/ui": "~0.31.0",
		"c8": "^7.13.0",
		"eslint-config-shared": "workspace:*",
		"js-yaml": "^4.1.0",
		"jsdom": "^22.0.0",
		"shared-config": "workspace:*",
		"svelte": "^3.59.1",
		"typescript": "^5.0.4",
		"validators": "workspace:*",
		"vite": "^4.3.5",
		"vitest": "~0.31.0"
	},
	"files": [
		"src/**/*.js",
		"src/**/*.svelte",
		"!src/fixtures",
		"!src/**/*.spec.js"
	],
	"exports": {
		"./src": "./src",
		"./string": "./src/string.js",
		"./story": "./src/story.js",
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./src/index.js"
		}
	},
	"dependencies": {
		"@lukeed/uuid": "^2.0.1",
		"d3-array": "^3.2.3",
		"d3-collection": "^1.0.7",
		"frontmatter": "^0.0.3",
		"ramda": "^0.29.0"
	}
}
