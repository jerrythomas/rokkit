		"eslint": "^8.24.0",
		"eslint-config-shared": "workspace:1.0.0",
		"js-yaml": "^4.1.0",
		"jsdom": "^19.0.0",
		"shared-config": "workspace:^1.0.0",
		"svelte": "^3.50.1",
		"typescript": "^4.8.4",
		"vite": "^3.1.3",
		"vitest": "~0.19.1"
	},
	"dependencies": {
		"@sparsh-ui/core": "workspace:^1.0.0",
		"d3-array": "^3.2.0",
		"d3-collection": "^1.0.7",
		"d3-scale": "^4.0.2",
		"ramda": "^0.28.0"
