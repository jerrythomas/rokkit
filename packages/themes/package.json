{
	"name": "@rokkit/themes",
	"version": "1.0.0-next.36",
	"description": "Themes for use with rokkit components.",
	"author": "Jerry Thomas <me@jerrythomas.name>",
	"license": "MIT",
	"svelte": "src/index.js",
	"module": "src/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepublishOnly": "tsc",
		"format": "prettier --write .",
		"lint": "eslint --fix .",
		"test:ct": "playwright test -c playwright.config.js",
		"test:ui": "vitest --ui",
		"test:ci": "vitest run",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"latest": "pnpm upgrade --latest && pnpm test:ci",
		"release": "tsc && pnpm publish --access public"
	},
	"devDependencies": {
		"@rokkit/core": "workspace:*",
		"@sveltejs/vite-plugin-svelte": "^2.4.3",
		"@vitest/coverage-v8": "^0.33.0",
		"@vitest/ui": "~0.33.0",
		"c8": "^8.0.1",
		"jsdom": "^22.1.0",
		"shared-config": "workspace:*",
		"svelte": "^4.1.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.7",
		"vitest": "~0.33.0"
	},
	"files": [
		"src/**/*.js",
		"src/**/*.svelte",
		"!src/fixtures",
		"!src/**/*.spec.js"
	],
	"exports": {
		"./modern.css": "./src/modern.css",
		"./material.css": "./src/material.css",
		"./minimal.css": "./src/minimal.css",
		"./rokkit.css": "./src/rokkit.css",
		"./prism.css": "./src/prism.css",
		"./palette.css": "./src/palette.css",
		"./markdown.css": "./src/markdown.css",
		"./package.json": "./src/package.json",
		".": {
			"import": "./src/index.js"
		}
	},
	"dependencies": {
		"rokkit": "workspace:latest"
	}
}
