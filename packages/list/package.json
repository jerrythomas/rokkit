{
	"name": "@sparsh-ui/list",
	"version": "1.0.0",
	"description": "Svelte list view components",
	"author": "Jerry Thomas <me@jerrythomas.name>",
	"license": "MIT",
	"main": "index.js",
	"svelte": "src/index.js",
	"module": "src/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepublishOnly": "tsc",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"test:ct": "playwright test -c playwright.config.js",
		"test:ci": "vitest run",
		"test:ui": "vitest --ui",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"release": "release-it --preRelease=beta",
		"upgrade": "pnpm upgrade"
	},
	"devDependencies": {
		"@playwright/experimental-ct-svelte": "^1.24.0",
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
		"@testing-library/svelte": "^3.1.3",
		"@types/d3-array": "^3.0.3",
		"@types/jsdom": "^16.2.14",
		"@unocss/core": "^0.44.5",
		"@unocss/preset-icons": "^0.44.5",
		"@unocss/preset-uno": "^0.44.5",
		"@unocss/reset": "^0.44.5",
		"@unocss/transformer-directives": "^0.44.5",
		"@vitest/ui": "~0.12.10",
		"c8": "^7.11.3",
		"eslint": "^7.32.0",
		"eslint-config-shared": "workspace:*",
		"js-yaml": "^4.1.0",
		"jsdom": "^19.0.0",
		"playwright": "^1.24.0",
		"shared-config": "workspace:^1.0.0",
		"svelte": "^3.49.0",
		"typescript": "^4.7.4",
		"unocss": "^0.44.5",
		"validators": "workspace:^1.0.0",
		"vite": "3",
		"vitest": "~0.12.10",
		"windicss": "^3.5.6"
	},
	"dependencies": {
		"@sparsh-ui/core": "workspace:*",
		"@sparsh-ui/icons": "workspace:1.0.0",
		"d3-array": "^3.2.0",
		"d3-collection": "^1.0.7"
	}
}
