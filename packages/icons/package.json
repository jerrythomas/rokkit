{
	"name": "@rokkit/icons",
	"version": "1.0.0-next.93",
	"description": "A minimal icon set for use in applications.",
	"author": "Jerry Thomas <me@jerrythomas.name>",
	"license": "MIT",
	"bin": "src/index.js",
	"main": "src/index.js",
	"module": "src/index.js",
	"type": "module",
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint --fix .",
		"build": "node src/index.js && pnpm format",
		"latest": "pnpm upgrade --latest",
		"release": "pnpm publish --access public"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@mdi/svg": "^7.4.47"
	},
	"devDependencies": {
		"@iconify/tools": "4.0.2",
		"@iconify/types": "^2.0.0",
		"iconify": "^1.4.0"
	},
	"files": [
		"lib",
		"src/**/*.js",
		"!src/**/*.spec.js"
	],
	"exports": {
		"./package.json": "./package.json",
		"./ui.json": "./lib/base.json",
		"./light.json": "./lib/light.json",
		"./solid.json": "./lib/solid.json",
		"./twotone.json": "./lib/twotone.json",
		"./components.json": "./lib/components.json",
		"./auth.json": "./lib/auth.json",
		".": "./lib/base.json"
	}
}
