{
	"name": "@rokkit/icons",
	"version": "1.0.0-next.29",
	"description": "A minimal icon set for use in applications.",
	"author": "Jerry Thomas <me@jerrythomas.name>",
	"license": "MIT",
	"bin": "src/index.js",
	"main": "src/index.js",
	"module": "src/index.js",
	"type": "module",
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint --fix .",
		"build": "node src/index.js && pnpm format",
		"latest": "pnpm upgrade --latest",
		"release": "pnpm publish --access public"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@mdi/svg": "^7.2.96"
	},
	"devDependencies": {
		"@iconify/tools": "3.0.1",
		"@iconify/types": "^2.0.0",
		"iconify": "^1.4.0"
	},
	"files": [
		"src/**/*.js",
		"!src/**/*.spec.js"
	],
	"exports": {
		"./lib": "./lib",
		"./package.json": "./package.json",
		"./states.json": "./lib/states.json",
		"./components.json": "./lib/components.json",
		"./auth.json": "./lib/auth.json",
		".": "./lib/states.json"
	}
}
