{
	"name": "@svelte-spice/icons",
	"version": "1.0.0",
	"description": "Icon repository.",
	"author": "Jerry Thomas <me@jerrythomas.name>",
	"license": "MIT",
	"main": "index.js",
	"svelte": "src/index.js",
	"module": "src/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"prettier": "@jerrythomas/prettier-config",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepublishOnly": "tsc",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"test:ui": "vitest --ui",
		"test": "vitest",
		"release": "release-it --preRelease=beta",
		"upgrade": "pnpm upgrade"
	},
	"exports": {
		"./package.json": "./package.json",
		"./heroicons/outline": "./src/heroicons/outline/index.js",
		"./heroicons/filled": "./src/heroicons/filled/index.js",
		"./index.d.ts": "./dist/index.d.ts"
	},
	"devDependencies": {
		"@iconify/iconify": "^2.2.1",
		"@iconify/json": "^2.1.131",
		"@iconify/tools": "^2.1.0",
		"@jerrythomas/prettier-config": "^1.0.0",
		"@sveltejs/vite-plugin-svelte": "^1.1.0",
		"@testing-library/svelte": "^3.2.2",
		"@vitest/ui": "~0.12.10",
		"eslint": "^8.26.0",
		"eslint-config-shared": "workspace:1.0.0",
		"shared-config": "workspace:^1.0.0",
		"svelte": "^3.52.0",
		"typescript": "~4.6.4",
		"vite": "^3.2.2",
		"vitest": "~0.12.10"
	}
}
