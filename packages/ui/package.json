{
  "name": "@rokkit/ui",
  "version": "1.0.0-next.115",
  "description": "Organisms are larger, more complex building blocks that are composed of multiple molecules",
  "author": "Jerry Thomas <me@jerrythomas.name>",
  "license": "MIT",
  "module": "src/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "bun clean && bun tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "build": "bun prepublishOnly"
  },
  "files": [
    "src/**/*.js",
    "src/**/*.svelte",
    "dist/**/*.d.ts"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./utils": "./src/lib/index.js",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.js",
      "svelte": "./src/index.js"
    }
  },
  "dependencies": {
    "@rokkit/actions": "workspace:latest",
    "@rokkit/core": "workspace:latest",
    "@rokkit/data": "workspace:latest",
    "@rokkit/input": "workspace:latest",
    "@rokkit/states": "workspace:latest",
    "d3-scale": "^4.0.2",
    "date-fns": "^4.1.0",
    "ramda": "^0.30.1",
    "typescript": "^5.8.3"
  }
}
