{
	"name": "skeleton",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "playwright test",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@jerrythomas/eslint-config-svelte": "^1.0.2",
		"@jerrythomas/prettier-config": "^1.0.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-vercel": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@unocss/core": "^0.44.7",
		"@unocss/preset-icons": "^0.44.7",
		"@unocss/preset-uno": "^0.44.7",
		"@unocss/reset": "^0.44.7",
		"@unocss/transformer-directives": "^0.44.7",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"typescript": "^4.9.3",
		"unocss": "^0.44.7",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/montserrat-alternates": "^4.5.11",
		"@fontsource/victor-mono": "^4.5.4",
		"@svelte-spice/core": "workspace:latest",
		"@svelte-spice/form": "workspace:latest",
		"@svelte-spice/themes": "workspace:latest"
	}
}
