{
	"name": "skeleton",
	"version": "1.0.0-next.8",
	"private": true,
	"prettier": "@jerrythomas/prettier-config",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "playwright test",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@jerrythomas/eslint-config-svelte": "^1.0.2",
		"@jerrythomas/prettier-config": "^1.0.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-vercel": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@unocss/core": "^0.44.7",
		"@unocss/preset-icons": "^0.44.7",
		"@unocss/preset-uno": "^0.44.7",
		"@unocss/reset": "^0.44.7",
		"@unocss/transformer-directives": "^0.44.7",
		"mdsvex": "^0.10.6",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-media": "^0.1.5",
		"typescript": "^4.9.3",
		"unocss": "^0.44.7",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/poppins": "^4.5.10",
		"@fontsource/victor-mono": "^4.5.4",
		"@rokkit/core": "workspace:latest",
		"@rokkit/form": "workspace:latest",
		"@rokkit/themes": "workspace:latest"
	}
