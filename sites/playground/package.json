{
	"name": "playground",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "vitest run",
		"test:ui": "vitest run ui",
		"test:watch": "vitest",
		"e2e": "playwright test",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@fontsource/montserrat-alternates": "^4.5.9",
		"@fontsource/victor-mono": "^4.5.4",
		"@observablehq/plot": "^0.4.3",
		"@playwright/test": "^1.23.1",
		"@sparsh-ui/base": "workspace:1.0.0",
		"@sparsh-ui/chart": "workspace:1.0.0",
		"@sparsh-ui/icons": "workspace:1.0.0",
		"@sparsh-ui/list": "workspace:1.0.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "1.0.0-next.78",
		"@sveltejs/kit": "next",
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
		"@testing-library/svelte": "^3.1.3",
		"@types/d3-time-format": "^4.0.0",
		"@vitest/ui": "~0.12.10",
		"c8": "^7.11.3",
		"d3-array": "^3.2.0",
		"d3-collection": "^1.0.7",
		"d3-format": "^3.1.0",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.1.0",
		"d3-time-format": "^4.1.0",
		"date-fns": "^2.28.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"ramda": "^0.28.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.8.0",
		"typescript": "~4.6.4",
		"vite": "^2.9.13",
		"vite-plugin-windicss": "^1.8.6",
		"vitest": "~0.12.10",
		"windicss": "^3.5.6"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/poppins": "^4.5.8"
	}
}
