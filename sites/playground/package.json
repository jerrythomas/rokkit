{
	"name": "playground",
	"version": "1.0.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test:ci": "vitest run",
		"test:ui": "vitest run ui",
		"test": "vitest",
		"e2e": "playwright test",
		"coverage": "vitest run --coverage",
		"upgrade": "pnpm upgrade"
	},
	"devDependencies": {
		"@fontsource/montserrat-alternates": "^4.5.10",
		"@fontsource/victor-mono": "^4.5.4",
		"@observablehq/plot": "^0.4.3",
		"@playwright/test": "^1.26.0",
		"@sparsh-ui/chart": "workspace:1.0.0",
		"@sparsh-ui/core": "workspace:^1.0.0",
		"@sparsh-ui/icons": "workspace:1.0.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/vite-plugin-svelte": "^1.0.7",
		"@testing-library/svelte": "^3.2.1",
		"@types/d3-time-format": "^4.0.0",
		"@vitest/ui": "~0.12.10",
		"c8": "^7.12.0",
		"d3-array": "^3.2.0",
		"d3-collection": "^1.0.7",
		"d3-format": "^3.1.0",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.1.0",
		"d3-time-format": "^4.1.0",
		"date-fns": "^2.29.3",
		"eslint": "^7.32.0",
		"eslint-config-shared": "workspace:1.0.0",
		"ramda": "^0.28.0",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.0",
		"typescript": "~4.6.4",
		"vite": "^3.1.3",
		"vite-plugin-windicss": "^1.8.8",
		"vitest": "~0.19.1",
		"windicss": "^3.5.6"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/poppins": "^4.5.9",
		"@lukeed/uuid": "^2.0.0"
	}
}
