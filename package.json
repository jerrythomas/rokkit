    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "vite": "^6.0.6",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
