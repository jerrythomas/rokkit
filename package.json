{
  "name": "rokkit",
  "version": "1.0.0-next.105",
  "module": "src/index.js",
  "type": "module",
  "npm": {
    "publish": false
  },
  "scripts": {
    "clean": "find . -name node_modules | xargs rm -rf",
    "test:watch": "vitest watch",
    "test:ci": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "lint": "eslint --fix",
    "lockfile": "pnpm install --lockfile-only",
    "bump": "bun run lockfile && bumpp --no-push",
    "bump:next": "bun run lockfile && bumpp --preid=next --no-push",
    "publish:all": "find ./packages -type d -depth 1 -exec sh -c 'cd \"{}\" && bun publish' \\;",
    "release": "bun run publish:all"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.6",
    "@testing-library/user-event": "^14.5.2",
    "@types/bun": "^1.1.16",
    "@vitest/coverage-v8": "^2.1.8",
    "bumpp": "^9.10.0",
    "eslint": "^9.18.0",
    "eslint-plugin-svelte": "^2.46.1",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rimraf": "^6.0.1",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "workspaces": [
    "./packages/*",
    "./sites/*"
  ],
  "dependencies": {}
}
